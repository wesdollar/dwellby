    "@twilio-paste/icons": "^11.0.0",
    "bcrypt": "^5.1.0",
    "cross-env": "^7.0.3",
    "isbot": "^3.6.5",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
